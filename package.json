		"@codemirror/theme-one-dark": "^6.1.3",
		"@codemirror/view": "^6.38.4",
		"@lezer/highlight": "^1.2.1",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
